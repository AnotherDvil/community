<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="ir_cron_obtain_results_prop" model="ir.cron" forcecreate="True">
        <field name="name">Community: Obtener resultados de las propuestas</field>
        <field name="model_id" ref="model_proposals"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">30</field>
        <field name="interval_type">minutes</field>
        <field name="active" eval="True"/>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
        <field name="state">code</field>
        <field name="code">model._cron_obtain_results()</field>
    </record>

</odoo>
